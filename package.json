{
  "name": "starter-h5",
  "version": "0.0.0",
  "packageManager": "pnpm@7.11.0",
  "scripts": {
    "dev": "vite --host --open",
    "build": "nr lint && nr type-check && nr build-only",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "build-only": "vite build && cp _redirects dist/_redirects",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "commit": "git-pick && git cz",
    "up": "taze major -I",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.2.0",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "dexie": "^3.2.2",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "qrcode": "^1.5.1",
    "sa-sdk-javascript": "^1.23.4",
    "three": "^0.144.0",
    "vant": "^3.6.2",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.0",
    "@chinbor/git-pick": "^0.0.8",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.59",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.5.0",
    "@types/three": "^0.144.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog-zh-emoji": "^0.0.5",
    "eslint": "^8.23.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.55.0",
    "taze": "^0.8.1",
    "typescript": "~4.8.3",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.1.3",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
